<h2>Risk Factors</h2>
<form action="#" data-interview-name="{{name}}">
    <fieldset>
        {{#if prettyName}}
            <legend>{{prettyName}}</legend>
        {{/if}}
        <p>Select each statement that applies to you.</p>
        <div id="errors" style="display: none;"></div>
        {{#each questions}}
            {{> riskFactorInterviews_interview-question}}
        {{/each}}
        <label for="none">
            None
            <input type="checkbox" name="choice" id="none" class="risk-factor-group" data-none=true data-group="risk-factor-group">
        </label>
        <button type="submit" id="submit-risk-factors" data-clickable="submitRiskFactors">Continue</button>
    </fieldset>
</form>